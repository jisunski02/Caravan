<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">


            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">


                <ImageView
                    android:id="@+id/vehicle_rent_photo"
                    android:layout_width="0dp"
                    android:layout_height="250dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"/>


            </androidx.constraintlayout.widget.ConstraintLayout>

                <androidx.cardview.widget.CardView
                    app:cardCornerRadius="5dp"
                    android:layout_marginStart="30dp"
                    android:layout_marginEnd="30dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:padding="10dp"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <TextView
                        android:id="@+id/packagedetails"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/poppins_bold"
                        android:text="COMPLETE PACKAGE"
                        android:textAllCaps="true"
                        android:textColor="@color/black"
                        android:textSize="15sp"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <TextView
                        android:id="@+id/tvPickup"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/poppins"
                        android:text="Pick-up Date: "
                        android:textAllCaps="false"
                        android:textSize="14sp"
                        app:layout_constraintStart_toStartOf="@+id/packagedetails"
                        app:layout_constraintTop_toBottomOf="@id/packagedetails" />

                    <TextView
                        android:id="@+id/pick_up_date"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/poppins"
                        android:text="Pick-up Date: "
                        android:textColor="@color/black"
                        android:textAllCaps="false"
                        android:textSize="14sp"
                        app:layout_constraintStart_toEndOf="@+id/tvPickup"
                        app:layout_constraintTop_toTopOf="@+id/tvPickup" />

                    <TextView
                        android:id="@+id/tvReturn"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/poppins"
                        android:text="Return Date: "
                        android:textAllCaps="false"
                        android:textSize="14sp"
                        app:layout_constraintStart_toStartOf="@+id/tvPickup"
                        app:layout_constraintTop_toBottomOf="@+id/tvPickup" />
                    <TextView
                        android:id="@+id/return_date"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/poppins"
                        android:text="Return Date: "
                        android:textColor="@color/black"
                        android:textAllCaps="false"
                        android:textSize="14sp"
                        app:layout_constraintStart_toEndOf="@+id/tvReturn"
                        app:layout_constraintTop_toTopOf="@+id/tvReturn" />

                    <TextView
                        android:id="@+id/tvMop"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/poppins"
                        android:text="Mode of Payment: "
                        android:textAllCaps="false"
                        android:textSize="14sp"
                        app:layout_constraintStart_toStartOf="@+id/tvReturn"
                        app:layout_constraintTop_toBottomOf="@+id/tvReturn" />

                    <TextView
                        android:id="@+id/mode_of_payment"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/poppins"
                        android:text="Return Date: "
                        android:textColor="@color/black"
                        android:textAllCaps="false"
                        android:textSize="14sp"
                        app:layout_constraintStart_toEndOf="@+id/tvMop"
                        app:layout_constraintTop_toTopOf="@+id/tvMop" />

                    <TextView
                        android:id="@+id/tvRentStatus"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/poppins"
                        android:text="Rent status: "
                        android:textAllCaps="false"
                        android:textSize="14sp"
                        app:layout_constraintStart_toStartOf="@+id/tvMop"
                        app:layout_constraintTop_toBottomOf="@+id/tvMop" />

                    <TextView
                        android:id="@+id/rent_status"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/poppins_bold"
                        android:text="Return Date: "
                        android:textColor="#FF5722"
                        android:textAllCaps="false"
                        android:textSize="14sp"
                        app:layout_constraintStart_toEndOf="@+id/tvRentStatus"
                        app:layout_constraintTop_toTopOf="@+id/tvRentStatus" />

                    <TextView
                        android:id="@+id/tvRentDays"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/poppins"
                        android:text="Rent Days: "
                        android:textAllCaps="false"
                        android:textSize="14sp"
                        app:layout_constraintStart_toStartOf="@+id/tvRentStatus"
                        app:layout_constraintTop_toBottomOf="@+id/tvRentStatus" />

                    <TextView
                        android:id="@+id/rent_days"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/poppins"
                        android:text="Return Date: "
                        android:textColor="#FF5722"
                        android:textAllCaps="false"
                        android:textSize="14sp"
                        app:layout_constraintStart_toEndOf="@+id/tvRentDays"
                        app:layout_constraintTop_toTopOf="@+id/tvRentDays" />

                    <TextView
                        android:id="@+id/total_price"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/poppins_bold"
                        android:text="Php 15,000.00"
                        android:textColor="#FF5722"
                        android:textAllCaps="false"
                        android:textSize="18sp"
                        app:layout_constraintStart_toStartOf="@+id/tvRentDays"
                        app:layout_constraintTop_toBottomOf="@+id/tvRentDays" />


                </androidx.constraintlayout.widget.ConstraintLayout>

                </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:id="@+id/cvCancelled"
                android:layout_marginStart="15dp"
                android:layout_marginEnd="15dp"
                android:layout_marginTop="15dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">


            <LinearLayout
                android:padding="10dp"
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">


                <TextView
                    android:fontFamily="@font/poppins_bold"
                    android:text="Reason for Cancellation: "
                    android:textSize="15sp"
                    android:textColor="@color/black"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>

                <TextView
                    android:id="@+id/tvReason"
                    android:fontFamily="@font/poppins_regular"
                    android:text="Reason for Cancellation: "
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>


            </LinearLayout>

            </androidx.cardview.widget.CardView>


            <TextView
